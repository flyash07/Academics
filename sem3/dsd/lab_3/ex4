:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55b43ec297b0 .scope module, "gb_tb" "gb_tb" 2 3;
 .timescale -9 -9;
P_0x55b43ec29940 .param/l "n" 0 2 4, +C4<00000000000000000000000000000011>;
v0x55b43ec3b5c0_0 .net "b", 2 0, v0x55b43ebeecf0_0;  1 drivers
v0x55b43ec3b6b0_0 .var "g", 2 0;
S_0x55b43ec299e0 .scope module, "uut" "gb" 2 8, 3 1 0, S_0x55b43ec297b0;
 .timescale -9 -9;
    .port_info 0 /INPUT 3 "g";
    .port_info 1 /OUTPUT 3 "b";
P_0x55b43ec2a470 .param/l "n" 0 3 2, +C4<00000000000000000000000000000011>;
v0x55b43ebeecf0_0 .var "b", 2 0;
v0x55b43ec3b3c0_0 .net "g", 2 0, v0x55b43ec3b6b0_0;  1 drivers
v0x55b43ec3b4a0_0 .var/i "i", 31 0;
E_0x55b43ec27c80 .event edge, v0x55b43ec3b3c0_0;
    .scope S_0x55b43ec299e0;
T_0 ;
    %wait E_0x55b43ec27c80;
    %load/vec4 v0x55b43ec3b3c0_0;
    %parti/s 1, 2, 3;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55b43ebeecf0_0, 4, 1;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x55b43ec3b4a0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x55b43ec3b4a0_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x55b43ebeecf0_0;
    %load/vec4 v0x55b43ec3b4a0_0;
    %addi 1, 0, 32;
    %part/s 1;
    %load/vec4 v0x55b43ec3b3c0_0;
    %load/vec4 v0x55b43ec3b4a0_0;
    %part/s 1;
    %xor;
    %ix/getv/s 4, v0x55b43ec3b4a0_0;
    %store/vec4 v0x55b43ebeecf0_0, 4, 1;
    %load/vec4 v0x55b43ec3b4a0_0;
    %subi 1, 0, 32;
    %store/vec4 v0x55b43ec3b4a0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55b43ec297b0;
T_1 ;
    %vpi_call 2 11 "$dumpfile", "gb.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55b43ec297b0 {0 0 0};
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x55b43ec3b6b0_0, 0, 3;
    %delay 20, 0;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x55b43ec3b6b0_0, 0, 3;
    %delay 20, 0;
    %pushi/vec4 6, 0, 3;
    %store/vec4 v0x55b43ec3b6b0_0, 0, 3;
    %delay 20, 0;
    %pushi/vec4 7, 0, 3;
    %store/vec4 v0x55b43ec3b6b0_0, 0, 3;
    %delay 20, 0;
    %vpi_call 2 18 "$display", "test complete" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
